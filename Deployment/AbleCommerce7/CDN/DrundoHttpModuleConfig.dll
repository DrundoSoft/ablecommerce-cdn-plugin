<DrundoHttpModuleConfig type="Drundo.HttpModule.Configuration.DrundoHttpModuleConfig, Drundo.HttpModule" >
  <CdnUrlName></CdnUrlName>
  <IsEnabled>false</IsEnabled>
  <IsSecure>false</IsSecure>
  <IsScriptEnabled>true</IsScriptEnabled>
  <ScriptRegex>&lt;script.+ src=\"(?!http)(?!https)(?!\.axd)([^\"]*)\"[^&gt;]*&gt;</ScriptRegex>
  <ScriptRegexElement>src="</ScriptRegexElement>
  <StyleRegex>&lt;link.+ href=\"(?!http)(?!https)([^\"]*)\"[^&gt;]*&gt;</StyleRegex>
  <StyleRegexElement>href="</StyleRegexElement>
  <ImageRegex>&lt;img.+src=\"(?!http)(?!https)(?!Captcha.ashx)(?!.(webcharts))([^\"]+)\"[^&gt;]+&gt;</ImageRegex>
  <ImageRegexElement>src="</ImageRegexElement>
</DrundoHttpModuleConfig>